#include <iostream>
#include <bits/stdc++.h>
using namespace std;

// op is output string;
// s is the input string;
// cc is character count in op;
// um is set/map;
// curr is index of current character in s;
// k is size of op;

void findways(string& s,int k,string& op,int curr,set<string>& um,int cc){
    if(curr == s.size()){
        if(cc == k){
            cout<<op<<" ";
            if(um.find(op) == um.end()){
                um.insert(op);
            }
        }
        return;
    }
    for(int i=0;i<k;i++){
        if(op[i] == '#'){
            op[i]=s[curr];
            findways(s,k,op,curr+1,um,cc+1);
            op[i]='#';
        }
    }
    findways(s,k,op,curr+1,um,cc);
}

void printways(string& s,int k){
    string op = string(k,'#');
    set<string>um;
    findways(s,k,op,0,um,0);
}

int main() {
	// your code goes here
	string input;
	int k;
	cin>>input;
	cin>>k;
	printways(input,k);
	return 0;
}
