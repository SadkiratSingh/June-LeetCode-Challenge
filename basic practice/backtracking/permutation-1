#include<iostream>
#include<bits/stdc++.h>
using namespace std;

void printVector(vector<int>& boxes){
    for(int x:boxes){
        cout<<x;
    }
    cout<<endl;
}

void findPermutations(vector<int>& boxes,int items,int ci){
    if(ci>items){
        printVector(boxes);
        return;
    }
    for(int i=0;i<boxes.size();i++){
        if(boxes[i]==0){
            boxes[i]=ci;
            findPermutations(boxes,items,ci+1);
            boxes[i]=0; 
        }
    }
}

void printPermutations(int b,int items){
    vector<int>boxes(b);
    findPermutations(boxes,items,1);
}

int main(){
    int boxesNumber;
    cin>>boxesNumber;
    int itemsNumber;
    cin>>itemsNumber;
    printPermutations(boxesNumber,itemsNumber);
    return 0;
}
