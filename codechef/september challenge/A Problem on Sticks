#include <bits/stdc++.h>
using namespace std;

int optimize(vector<int>& holder,int arr_size){
    // use set as an optimizer
    set<int>optimizer;
    for(int x:holder){
        if(x!=0)
            optimizer.insert(x);
    }
    
    int op=0;// count operations
    while(!optimizer.empty()){
        int longest_stick=*(--optimizer.end());
        optimizer.erase(--optimizer.end());
        int cutter,cut_stick;
	
	//decide the cutter size.
        if(optimizer.size()>=1)
            cutter=*(--optimizer.end());
        else
            cutter=0;
        cut_stick=longest_stick-cutter;
	
	//move the cutter
        for(int i=0;i<arr_size;i++){
            if(holder[i]==longest_stick)
                holder[i]-=cut_stick;
        }
        op++;
    }
    return op;
}

int main() {
	// your code goes herE
	int T;
	cin>>T;
	while(T>0){
    	int arr_size;
    	cin>>arr_size;
    	vector<int>holder(arr_size);
    	for(int i=0;i<arr_size;i++){
    	    int stick;
    	    cin>>stick;
    	    holder[i]=stick;
    	}
    	cout<<optimize(holder,arr_size)<<endl;
    	T--;
	}
	return 0;
}
