#include <iostream>
using namespace std;

int ISS(int* set,int sum,int n,int** dp){
    for(int i=1;i<=sum;i++){
        dp[0][i]=0;
    }
    for(int i=0;i<=n;i++){
        dp[i][0]=1;
    }
    for(int r=1;r<=n;r++){
        for(int c=1;c<=sum;c++){
            if(set[r-1]>c){
                dp[r][c]=dp[r-1][c];
            }
            else{
                dp[r][c]=dp[r-1][c]||dp[r-1][c-set[r-1]];
            }
        }
    }
    return dp[n][sum];
}

int main() {
	// your code goes here
	int set[]={3, 34, 4, 12, 5, 2};
	int n=sizeof(set)/sizeof(int);
	int sum=30;
	int** dp=new int*[n+1];
	for(int i=0;i<=n;i++){
	    dp[i]=new int[sum+1];
	}
	cout<<ISS(set,sum,n,dp);
	return 0;
}
