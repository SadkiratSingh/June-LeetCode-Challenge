#include <iostream>
using namespace std;

int CC(int* set,int N,int n,int** dp){
    for(int i=1;i<=N;i++){
        dp[0][i]=0;
    }
    for(int i=0;i<=n;i++){
        dp[i][0]=1;
    }
    for(int r=1;r<=n;r++){
        for(int c=1;c<=N;c++){
            if(set[r-1]>c){
                dp[r][c]=dp[r-1][c];
            }
            else{
                dp[r][c]=dp[r][c-set[r-1]]+dp[r-1][c];
            }
        }
    }
    return dp[n][N];
}


int main() {
	// your code goes here
	int set[]={1,2,3};
	int N=4;
	int n=sizeof(set)/sizeof(int);
	int** dp=new int*[n+1];
	for(int i=0;i<=n;i++){
	    dp[i]=new int[N+1];
	}
	cout<<CC(set,N,n,dp);
	return 0;
}
