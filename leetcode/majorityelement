class Solution {
public:
    int majorityElement(vector<int>& nums) {
        unordered_map<int,int>um;
        int majority=floor(nums.size()/2);
        for(int i=0;i<nums.size();i++){
            um[nums[i]]+=1;
        }
        for(auto x:um){
            if(x.second>majority){
                return x.first;
            }
        }
        return 0;
    }
};
