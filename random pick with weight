import random
class Solution:
    
    def __init__(self, w: List[int]):
        self.my_list=w
        self.new_list=[]
        sum=0
        for num in self.my_list:
            sum+=num
            self.new_list+=[sum]

    def pickIndex(self) -> int:
        max_n=self.new_list[len(self.new_list)-1]-1
        r_num=random.randint(0,max_n);
        for i in range(len(self.new_list)):
            if r_num<self.new_list[i]:
                return i
        return -1
